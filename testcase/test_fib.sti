int fib (int x)
{
	int tmp;
	if (x < 2){
		tmp = 1;
	}
	else{ 
		tmp = fib(x-1) + fib(x-2);
	}

	return tmp;
}

void main()
{
	int i = 0;
	while (i <= 5) {
		std << fib(i) +  "\n";
		i = i + 1;
	}
